{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "images/delicious32.png",
      "default_title": "This page is Delicious!"
   },
   "description": "Bookmarks your favorite links to Delicious.com, with a quick access, user-friendly interface.",
   "icons": {
      "128": "images/delicious128.png",
      "24": "images/delicious24.png",
      "32": "images/delicious32.png"
   },
   "name": "Delicious Bookmarks",
   "permissions": [ "tabs", "bookmarks" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "3.0",
   "manifest_version": 2
}
